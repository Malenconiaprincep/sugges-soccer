# Strapi Cloud 配置
# 请将以下值替换为您的 Strapi Cloud 实例信息

# Strapi Cloud 实例 URL
REACT_APP_STRAPI_URL=https://your-project-name.strapi.cloud

# Strapi Cloud API Token
# 您可以在 Strapi Cloud 控制台中找到此 token
REACT_APP_STRAPI_TOKEN=your-strapi-cloud-token-here

# Strapi 应用配置
APP_KEYS=key1,key2,key3,key4
HOST=0.0.0.0
PORT=1337

# 数据库配置 (Strapi Cloud 会自动配置)
DATABASE_CLIENT=postgres
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=strapi
DATABASE_USERNAME=strapi
DATABASE_PASSWORD=strapi
DATABASE_SSL=false
